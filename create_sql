CREATE TABLE IF NOT EXISTS public.monthly_kouza1_payment
(
    id serial,
    date date NOT NULL,
    price integer NOT NULL,
    description text COLLATE pg_catalog."default",
    category_id bigint NOT NULL,
    paycategory_id bigint NOT NULL);

